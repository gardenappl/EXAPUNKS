GRAB 300
COPY F X
DROP
LINK 800
REPL REPL2
REPL REPL3

MARK REPL1
  COPY 800 T
  REPL GO
  COPY 801 T
  JUMP GO

MARK REPL2
  COPY 802 T
  REPL GO
  COPY 803 T
  JUMP GO

MARK REPL3
  COPY 804 T
  REPL GO
  COPY 805 T
MARK GO
  LINK T
  @REP 3
  KILL
  @END
  COPY 200 T

MARK MODIFY
  GRAB T
  SEEK 2
  COPY X F
  MARK MODIFY_SUB
    COPY F T
    REPL MODIFY
    TEST EOF
    FJMP MODIFY_SUB
  HALT
  