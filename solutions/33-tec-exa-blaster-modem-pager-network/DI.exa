GRAB 301
LINK 800
COPY 8 X

; 100 CYCLES PER ITER
MARK DIAL
  @REP 11
  COPY F #DIAL
  @END
  SUBI X 1 X
  REPL ENTER
  COPY 41 T
  ; 82 CYCLES
  MARK DIAL_WAIT
    SUBI T 1 T
    TJMP DIAL_WAIT
  NOOP
  COPY -1 #DIAL
  TEST X > 0
  TJMP DIAL
WIPE
HALT
  
MARK ENTER
  LINK 800
  MARK RECV_MSG
    COPY M #DATA
    COPY M T
    FJMP RECV_MSG
  MULI X 50 T
  FJMP RESONATE
  ; 100*N CYCLES
  MARK ENTER_WAIT
    SUBI T 1 T
    TJMP ENTER_WAIT
  MARK RESONATE
    COPY 1 #PAGE
  
  