GRAB 300
COPY F X
WIPE
LINK 800
REPL READ_DATE
  GRAB 200
  MARK READ_LINE
    TEST F = X
    TJMP FOUND
      SEEK 2
      JUMP READ_LINE
    MARK FOUND
      COPY F X
      SEEK -1
      COPY 0 F
      COPY F T
      SEEK -1
      COPY 0 F
      DROP
  GRAB 201
  SEEK 9999
  COPY M F
  COPY M F
  COPY X F
  COPY T F
  DROP
  HALT
MARK READ_DATE
  LINK 801
  COPY #DATE M
  COPY X M
  HALT