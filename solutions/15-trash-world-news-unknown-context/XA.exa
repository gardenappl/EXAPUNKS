GRAB 300
LINK 800
REPL OUTBOX
LINK 799
COPY F X
REPL MODIFY_FILE

MARK READ_MAP
  COPY F M
  TEST EOF
  COPY T M
  TJMP READ_MAP_END
    COPY F M
    JUMP READ_MAP
  MARK READ_MAP_END
    WIPE
    HALT
  
MARK MODIFY_FILE
  GRAB 212
  MARK CHECK_WORD
    TEST F = X
    FJMP CHECK_WORD
    SEEK -1
    COPY M F
    COPY M T
    TJMP MODIFY_END
      SEEK -9999
      COPY M X
      JUMP CHECK_WORD
    MARK MODIFY_END
      DROP
      HALT
  
MARK OUTBOX
  GRAB 200
  LINK 800
  DROP
  HALT