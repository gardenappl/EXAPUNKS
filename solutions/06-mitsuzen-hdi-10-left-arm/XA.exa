LINK 800
REPL READ_LOOP
 LINK 1
 LINK 1
 LINK 1
 LINK 1
 MARK WRITE_LOOP
  COPY M X
  TEST X > 50
  TJMP WRITE_HIGH
   COPY X #NERV
   JUMP WRITE_LOOP
  MARK WRITE_HIGH
   COPY 50 #NERV
   JUMP WRITE_LOOP
MARK READ_LOOP
 COPY #NERV X
 TEST X < -120
 TJMP READ_LOW
  COPY X M
  JUMP READ_LOOP
 MARK READ_LOW
  COPY -120 M
  JUMP READ_LOOP
